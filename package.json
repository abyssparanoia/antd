{
  "name": "antd",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.25.2",
    "axios": "^0.19.0",
    "connected-react-router": "^6.6.0",
    "prism-react-renderer": "^1.0.2",
    "react": "^16.9.0",
